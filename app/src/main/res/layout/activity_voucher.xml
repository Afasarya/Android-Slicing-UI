<LinearLayout
    xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    android:background="#FFFFFFFF"
    android:clipChildren="false"
    android:clipToPadding="false"
    android:orientation="vertical"
    android:layout_width="match_parent"
    android:layout_height="match_parent">
    <ScrollView
        android:fillViewport="true"
        android:id="@+id/r181bxzm9n0z"
        android:layout_width="match_parent"
        android:layout_height="match_parent"
        android:layout_weight="1"
        android:background="#FF1E1E1E"
        android:clipChildren="false"
        android:clipToPadding="false">
        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="match_parent"
            android:clipChildren="false"
            android:clipToPadding="false"
            android:orientation="vertical">
            <LinearLayout
                android:id="@+id/rgjiqxwipoip"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="#FF1E1E1E"
                android:paddingVertical="16dp"
                android:paddingHorizontal="22dp"
                android:layout_marginBottom="5dp"
                android:orientation="horizontal"/>
            <LinearLayout
                android:id="@+id/r65qvo7z0rlx"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:layout_marginBottom="20dp"
                android:layout_marginHorizontal="24dp"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/ro2o9d4b6nnl"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginEnd="61dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@drawable/arrowmenu"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/rsuqbqoe50fh"
                    android:textColor="#FFFFFF"
                    android:textSize="15sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="My Voucher/Coupon"
                    />
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rvkizzzvmn07"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:background="@drawable/s1affffffsw1cr10bff2c2c2c"
                android:paddingVertical="7dp"
                android:paddingLeft="59dp"
                android:paddingRight="43dp"
                android:layout_marginBottom="150dp"
                android:layout_marginHorizontal="20dp"
                android:orientation="horizontal">
                <TextView
                    android:id="@+id/rsrfljxlztg"
                    android:textColor="#FF6666"
                    android:textSize="12sp"
                    android:layout_marginEnd="29dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:layout_height="wrap_content"
                    android:layout_width="wrap_content"
                    android:text="VOUCHER (0)"
                    />
                <ImageView
                    android:id="@+id/r0nzfagzaf2v"
                    android:layout_width="1dp"
                    android:layout_height="26dp"
                    android:layout_marginEnd="50dp"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
                <TextView
                    android:id="@+id/rs95wm97s4po"
                    android:textColor="#FFFF"
                    android:textSize="12sp"
                    android:layout_height="wrap_content"
                    android:layout_width="match_parent"
                    android:layout_weight="1"
                    android:text="COUPON (0)"
                    />
            </LinearLayout>

            <LinearLayout
                android:id="@+id/rjmm06pcyfcm"
                android:layout_width="wrap_content"
                android:layout_height="70dp"
                android:layout_marginHorizontal="112dp"
                android:layout_marginBottom="25dp"
                android:background="@drawable/cr75bffe6e6e6"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical"
                android:paddingHorizontal="37dp">

                <LinearLayout
                    android:id="@+id/r7g7qy8r9iu"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:background="@drawable/couponmen"
                    android:orientation="vertical"></LinearLayout>

                <RelativeLayout
                    android:id="@+id/rezosn4adpum"
                    android:layout_width="match_parent"
                    android:layout_height="wrap_content"
                    android:layout_marginTop="37dp"
                    android:clipChildren="false"
                    android:clipToPadding="false">

                    <LinearLayout
                        android:layout_width="match_parent"
                        android:layout_height="match_parent"
                        android:clipChildren="false"
                        android:clipToPadding="false"
                        android:orientation="vertical"
                        android:gravity="center">

                    </LinearLayout>


                    <RelativeLayout
                        android:id="@+id/rgmhgzafhd8_rl"
                        android:layout_width="30dp"
                        android:layout_height="30dp"
                        android:layout_alignParentRight="true"
                        android:layout_alignParentBottom="true"
                        android:layout_marginEnd="-5dp"
                        android:layout_marginBottom="2dp"
                        android:clipChildren="false"
                        android:clipToPadding="false" />
                </RelativeLayout>
            </LinearLayout>
            <TextView
                android:id="@+id/r6r3ootzqrbt"
                android:textColor="#FFFFFF"
                android:textSize="13sp"
                android:layout_marginBottom="264dp"
                android:layout_marginHorizontal="96dp"
                android:layout_height="wrap_content"
                android:layout_width="match_parent"
                android:layout_weight="1"
                android:text="You donâ€™t have any voucher at the moment"
                />
            <LinearLayout
                android:id="@+id/r1r8lcbk8yoq"
                android:layout_width="match_parent"
                android:layout_height="1dp"
                android:background="#4DFFFFFF"
                android:layout_marginBottom="6dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="vertical">
            </LinearLayout>
            <LinearLayout
                android:id="@+id/rwficewl4c9s"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:gravity="center_vertical"
                android:paddingVertical="13dp"
                android:paddingHorizontal="25dp"
                android:layout_marginHorizontal="24dp"
                android:clipChildren="false"
                android:clipToPadding="false"
                android:orientation="horizontal">
                <ImageView
                    android:id="@+id/rnnxbxt98iua"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@drawable/home"
                    android:scaleType="fitXY"
                    />
                <LinearLayout
                    android:id="@+id/rernx6893qm"
                    android:layout_width="match_parent"
                    android:layout_height="match_parent"
                    android:layout_weight="0.3"
                    android:clipChildren="false"
                    android:clipToPadding="false"
                    android:orientation="vertical">
                </LinearLayout>
                <ImageView
                    android:id="@+id/rs78sbdlx7b"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginEnd="60dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
                <ImageView
                    android:id="@+id/rp4bc0z1krp8"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:layout_marginEnd="60dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
                <ImageView
                    android:id="@+id/r8ctdynpxase"
                    android:layout_width="24dp"
                    android:layout_height="24dp"
                    android:clickable="true"
                    android:focusable="true"
                    android:src="@mipmap/ic_launcher"
                    android:scaleType="fitXY"
                    />
            </LinearLayout>
        </LinearLayout>
    </ScrollView>
</LinearLayout>
